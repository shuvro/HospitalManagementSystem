<script type="text/javascript">
    $('#select_department').on('change', function() {
        var selected = $(this).val();
        //alert(selected);

        $.ajax({
            url: '<?php echo base_url();?>ReceptionistGet/dept_doctor/' + selected,
            //data: {'selected': selected},
            type: 'GET',
            dataType: 'json',
            error: function() {
                //alert("Sorry...Something goes Wrong");
            },
            success: function(data) {

                    $('#doctor-list').empty();

                    for (var i = 0; i < data.length; i++) {
                        $("#doctor-list").append("<option value='" + data[i]['user_id'] + "'>" + data[i]['name'] + "</option>");
                    }
                } // End of success function of ajax form
        }); // End of ajax call
    });
</script>
<script type="text/javascript">
    $(document).on('click', '#prescribe', function() {
        var edit_id = $(this).closest('tr').children()[1].textContent;
        $('#appointment_id').val(edit_id);
        //alert(edit_id);
        $("#add-prescription").modal("show");
    });

    $(document).on('click', '#details', function() {

        var edit_id = $(this).closest('tr').children()[1].textContent;
        //alert(edit_id);

        //         $(".modal #prescription_id").val(edit_id);
        var temp;
        //    $("#appointment_modal").modal("show");

        $.ajax({

            url: '<?php echo base_url(); ?>receptionist/get_reports',
            data: {
                'edit_id': edit_id
            },
            type: 'POST',
            dataType: 'json',

            error: function() {
                alert("Nothing Prescribe");
            },

            success: function(data) {
                    if (data.reports.length > 0) {
                        for (var i = 0; i < data.reports.length; i++) {
                            temp += '<tr><th scope="row">' + (i + 1) + '</th><td>' + data.reports[i].report_type + '</td><td><img src="<?php echo base_url(); ?>others/images/x-ray.jpg" height="50px" width="107px"><a href="https://docs.google.com/viewer?url=<?php echo base_url() ?>/uploads/report/' + data.reports[i].file_name + '" target="_blank"><button class="btn btn-primary" style="margin-top: 10px;"><i class="fa fa-download" aria-hidden="true"></i>Download</button></a></td><td>' + data.reports[i].description + '</td><td>' + data.reports[i].timestamp + '</td><td>' + data.reports[i].laboratorist_name + '</td></tr>';
                        }

                        $("#all_reports").empty();
                        $("#all_reports").append(temp);
                    } else {
                        $("#all_reports").empty();
                        $("#all_reports").append('No Reports');
                    }
                    $("#medication").html(data.prescription_details[0].medication);
                    $("#doctor-advise").html(data.prescription_details[0].advise);
                    $("#appointment_modal").modal("show");

                } // End of success function of ajax form
        }); // End of ajax call
    });
</script>

